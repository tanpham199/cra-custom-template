{
  "name": "cra-custom-template",
  "version": "1.0.0",
  "private": true,
  "homepage": "/",
  "scripts": {
    "build:prod": "env-cmd -f .env.production node scripts/build.js",
    "build:prod-hydrate": "env-cmd -f .env.production.hydrate node scripts/build.js && react-snap",
    "build:staging": "env-cmd -f .env.staging node scripts/build.js",
    "build:staging-hydrate": "env-cmd -f .env.staging.hydrate node scripts/build.js && react-snap",
    "format": "prettier --write src/**/*.{ts,tsx,scss,module.scss}",
    "lint:css": "stylelint src/**/*.{scss,module.scss}",
    "lint:js": "eslint src/**/*.{ts,tsx}",
    "serve": "nodemon server.js",
    "start:dev": "node scripts/start.js",
    "test": "jest --config=jest.config.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test && lint-staged"
    }
  },
  "lint-staged": {
    "*.(ts|tsx|scss|module.scss)": [
      "yarn format"
    ],
    "*.(ts|tsx)": [
      "yarn lint:js"
    ],
    "*.(scss|module.scss)": [
      "yarn lint:css"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "web-vitals": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/glob": "^8.0.0",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "babel-jest": "^29.3.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-named-asset-import": "^0.3.8",
    "babel-preset-react-app": "^10.0.1",
    "bfj": "^7.0.2",
    "browserslist": "^4.21.4",
    "camelcase": "^6.2.1",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "css-loader": "^6.7.2",
    "css-minimizer-webpack-plugin": "^3.2.0",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.29.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-webpack-plugin": "^3.2.0",
    "express": "^4.18.2",
    "file-loader": "^6.2.0",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-resolve": "^29.3.1",
    "jest-watch-typeahead": "^2.2.1",
    "lint-staged": "^13.0.4",
    "mini-css-extract-plugin": "^2.7.2",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.19",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^7.0.1",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^7.8.3",
    "prettier": "^2.8.0",
    "prompts": "^2.4.2",
    "react-app-polyfill": "^3.0.0",
    "react-dev-utils": "^12.0.1",
    "react-refresh": "^0.14.0",
    "react-snap": "^1.23.0",
    "resolve": "^1.22.1",
    "resolve-url-loader": "^4.0.0",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "semver": "^7.3.8",
    "source-map-loader": "^3.0.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.16.0",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-scss": "^4.3.0",
    "tailwindcss": "^3.2.4",
    "terser-webpack-plugin": "^5.3.6",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-manifest-plugin": "^4.0.2",
    "workbox-webpack-plugin": "^6.5.4"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
